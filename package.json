{
  "name": "vuebx",
  "version": "1.0.0",
  "description": "a lightweight state management solution for vuejs",
  "main": "./dist/vuebx.umd.js",
  "module": "./dist/vuebx.es.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naecoo/vuebx.git"
  },
  "keywords": [
    "vue",
    "vuex"
  ],
  "bugs": {
    "url": "https://github.com/naecoo/vuebx/issues"
  },
  "homepage": "https://github.com/naecoo/vuebx#readme",
  "scripts": {
    "test": "npx tape ./test/*",
    "build:es": "rollup -c ./build/es.config.js",
    "build:umd": "rollup -c ./build/umd.config.js",
    "build": "npm run build:es && npm run build:umd"
  },
  "author": "naecoo",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.11",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "jest-worker": "^24.6.0",
    "rollup": "^1.11.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-terser": "^4.0.4",
    "tape": "^4.10.1",
    "terser": "^3.17.0"
  }
}
